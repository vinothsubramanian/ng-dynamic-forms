{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@ss-dynamic-forms/*": [
        "./dist/@ss-dynamic-forms/*"
      ]
    },
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2017",
      "dom"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es5",
    "typeRoots": [],
    "types": []
  }
}